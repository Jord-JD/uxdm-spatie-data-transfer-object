{
  "name": "jord-jd/uxdm-spatie-data-transfer-object",
  "description": "Spatie Data Transfer Object package for the UXDM data migrator",
  "type": "library",
  "authors": [
    {
      "name": "Jordan Hall",
      "email": "jordan@hall05.co.uk"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "fakerphp/faker": "^1.23",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "jord-jd/uxdm": "^6.0",
    "spatie/data-transfer-object": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "JordJD\\uxdm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JordJD\\uxdm\\TestClasses\\": "tests/Unit/includes/TestClasses/"
    }
  },
  "license": "LGPL-3.0-only",
  "replace": {
    "divineomega/uxdm-spatie-data-transfer-object": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  }
}
